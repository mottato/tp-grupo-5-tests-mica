{{#partial "contenido" }}
<body>
  <div class="administrar-usuarios">
    <h1 class="cargar-text">Informes Incidentes</h1>
    <div class="mb-4">
              <form method="POST" action="generar-informes-incidentes" >
             <button type="submit"  class="btn btn-primary">Generar informe</button>
              </form>
    </div>
       <div class="table-responsive">
           <table class="table table-striped table-hover">
               <tr>
                    <th scope="col">Entidad</th>
                    <th scope="col">Promedio (en segundos)</th>
               </tr>
               <tbody>
               {{#each entidades}}
                   <tr>
                       <td>{{this.entidad.leyenda}}</td>
                       <td>{{this.promedio}}</td>
                       <td>--</td>
                   </tr>
               {{/each}}
               </tbody>
           </table>
    </div>
  </div>
  {{/partial}}
  {{> templates/base }}